{
  "name": "EpicChainGraphicalWizard",
  "publisher": "EpicChainGraphicalWizard",
  "displayName": "EpicChain Graphical Wizard",
  "description": "The EpicChain Graphical Wizard Extension offers a user-friendly interface for tracking and overseeing your development tasks, ensuring a streamlined workflow from start to finish.",
  "version": "1.0.0",
  "icon": "resources/epicchain.png",
  "galleryBanner": {
    "color": "#242424",
    "theme": "dark"
  },
  "author": "xmoohad",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/epicchainlabs/epicchain-graphical-wizard/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicchainlabs/epicchain-graphical-wizard.git"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": "limited",
      "description": "Trust is required to build, deploy and debug smart contracts"
    }
  },
  "activationEvents": [
    "onCommand:epicchain-graphical-wizard.customizeServerList",
    "onCommand:epicchain-graphical-wizard.express.contractDeploy",
    "onCommand:epicchain-graphical-wizard.express.create",
    "onCommand:epicchain-graphical-wizard.express.createCheckpoint",
    "onCommand:epicchain-graphical-wizard.express.exploreStorage",
    "onCommand:epicchain-graphical-wizard.express.reset",
    "onCommand:epicchain-graphical-wizard.express.restoreCheckpoint",
    "onCommand:epicchain-graphical-wizard.express.run",
    "onCommand:epicchain-graphical-wizard.express.runAdvanced",
    "onCommand:epicchain-graphical-wizard.express.transfer",
    "onCommand:epicchain-graphical-wizard.express.walletCreate",
    "onCommand:epicchain-graphical-wizard.epicchain.contractDeploy",
    "onCommand:epicchain-graphical-wizard.epicchain.invokeContract",
    "onCommand:epicchain-graphical-wizard.epicchain.newContract",
    "onCommand:epicchain-graphical-wizard.epicchain.walletCreate",
    "onCommand:epicchain-graphical-wizard.tracker.openContract",
    "onCommand:epicchain-graphical-wizard.tracker.openTracker",
    "onCommand:epicchain-graphical-wizard.tracker.openWallet",
    "onCustomEditor:epicchain-graphical-wizard.express.epicchain-invoke-json",
    "onView:epicchain-graphical-wizard.views.blockchains",
    "onView:epicchain-graphical-wizard.views.quickStart",
    "workspaceContains:**/*.nef",
    "workspaceContains:**/*.avm",
    "workspaceContains:**/*.epicchain-express",
    "workspaceContains:**/*.epicchain-invoke.json",
    "workspaceContains:**/*.epicchain-wallet.json"
  ],
  "keywords": [
    "EpicChain",
    "Blockchain"
  ],
  "main": "./dist/extension/index.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "epicchain-graphical-wizard.epicchain.epicchain-invoke-json",
        "displayName": "EpicChain Express Contract Invoke File Editor",
        "selector": [
          {
            "filenamePattern": "*.epicchain-invoke.json"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "epicchain-graphical-wizard.express.contractDeploy",
        "title": "Deploy contract to EpicChain Express",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.create",
        "title": "Create private blockchain",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.createCheckpoint",
        "title": "Create checkpoint",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.customCommand",
        "title": "Run custom EpicChain Express command",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.exploreStorage",
        "title": "Explore contract storage",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.reset",
        "title": "Reset blockchain",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.restoreCheckpoint",
        "title": "Restore checkpoint",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.run",
        "title": "Start blockchain",
        "category": "EpicChain Graphical Wizard",
        "icon": {
          "light": "resources/light/play.svg",
          "dark": "resources/dark/play.svg"
        }
      },
      {
        "command": "epicchain-graphical-wizard.express.runAdvanced",
        "title": "Start blockchain (with custom options)...",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.stop",
        "title": "Stop blockchain",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.transfer",
        "title": "Transfer assets",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.express.walletCreate",
        "title": "Create wallet",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.epicchain.contractDeploy",
        "title": "Deploy contract",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.epicchain.invokeContract",
        "title": "Invoke contract",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.epicchain.newContract",
        "title": "Create contract",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.epicchain.walletCreate",
        "title": "Create wallet",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.tracker.openContract",
        "title": "Show smart contract information",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.tracker.openTracker",
        "title": "Open EpicChain Graphical Wizard",
        "category": "EpicChain Graphical Wizard",
        "icon": {
          "light": "resources/light/open.svg",
          "dark": "resources/dark/open.svg"
        }
      },
      {
        "command": "epicchain-graphical-wizard.tracker.openWallet",
        "title": "Show wallet",
        "category": "EpicChain Graphical Wizard"
      },
      {
        "command": "epicchain-graphical-wizard.connect",
        "title": "Connect to a EpicChain Graphical Wizard blockchain",
        "category": "EpicChain Graphical Wizard Extension"
      },
      {
        "command": "epicchain-graphical-wizard.customizeServerList",
        "title": "Customize blockchain list",
        "category": "EpicChain Graphical Wizard Extension"
      },
      {
        "command": "epicchain-graphical-wizard.disconnect",
        "title": "Disconnect from current EpicChain blockchain",
        "category": "EpicChain Graphical Wizard Extension"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "epicchain-graphical-wizard.express.contractDeploy",
          "when": "isFileSystemResource && resourceExtname == .nef"
        },
        {
          "command": "epicchain-graphical-wizard.epicchain.contractDeploy",
          "when": "isFileSystemResource && resourceExtname == .nef"
        }
      ],
      "view/item/context": [
        {
          "command": "epicchain-graphical-wizard.express.contractDeploy",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.customCommand",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.createCheckpoint",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.exploreStorage",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.reset",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.restoreCheckpoint",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.run",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express",
          "group": "inline"
        },
        {
          "command": "epicchain-graphical-wizard.express.run",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.runAdvanced",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.stop",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.transfer",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.express.walletCreate",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.epicchain.contractDeploy",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem != express"
        },
        {
          "command": "epicchain-graphical-wizard.epicchain.invokeContract",
          "when": "view == epicchain-graphical-wizard.views.blockchains && viewItem == express"
        },
        {
          "command": "epicchain-graphical-wizard.tracker.openTracker",
          "when": "view == epicchain-graphical-wizard.views.blockchains",
          "group": "inline"
        },
        {
          "command": "epicchain-graphical-wizard.tracker.openTracker",
          "when": "view == epicchain-graphical-wizard.views.blockchains"
        }
      ],
      "view/title": [
        {
          "command": "epicchain-graphical-wizard.customizeServerList",
          "when": "view == epicchain-graphical-wizard.views.blockchains"
        },
        {
          "command": "epicchain-graphical-wizard.express.create",
          "when": "view == epicchain-graphical-wizard.views.blockchains"
        },
        {
          "command": "epicchain-graphical-wizard.epicchain.newContract",
          "when": "view == epicchain-graphical-wizard.views.contracts"
        },
        {
          "command": "epicchain-graphical-wizard.epicchain.walletCreate",
          "when": "view == epicchain-graphical-wizard.views.wallets"
        }
      ]
    },
    "views": {
      "epicchain-graphical-wizard-mainView": [
        {
          "id": "epicchain-graphical-wizard.views.blockchains",
          "name": "Blockchains"
        },
        {
          "id": "epicchain-graphical-wizard.views.wallets",
          "name": "Wallets"
        },
        {
          "id": "epicchain-graphical-wizard.views.contracts",
          "name": "Smart contracts"
        },
        {
          "id": "epicchain-graphical-wizard.views.quickStart",
          "name": "Quick Start",
          "type": "webview"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "epicchain-graphical-wizard-mainView",
          "title": "EpicChain Graphical Wizard",
          "icon": "resources/n3-logo.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile-prod && npm run bundle-nxp",
    "bundle-nxp": "npm run bundle-nxp-download && npm run bundle-nxp-extract",
    "bundle-nxp-download": "shx rm -rf deps/nxp && shx mkdir -p deps/nxp && nwget \"https://github.com/neo-project/neo-express/releases/download/3.5.20/neo.Express.3.5.20.nupkg\" -O deps/nxp/nxp.nupkg",
    "bundle-nxp-extract": "cd deps/nxp && extract-zip nxp.nupkg",
    "compile": "npm run compile-ext && npm run compile-panel",
    "compile-ext": "webpack --config src/extension/webpack.config.js --mode development",
    "compile-panel": "webpack --config src/panel/webpack.config.js --mode development",
    "compile-prod": "npm run compile-prod-ext && npm run compile-prod-panel",
    "compile-prod-ext": "webpack --config src/extension/webpack.config.js --mode production",
    "compile-prod-panel": "webpack --config src/panel/webpack.config.js --mode production",
    "package": "npm run compile-prod && npm run bundle-nxp && vsce package",
    "setversion": "gulp setversion",
    "vscode-start": "npm install && npm run bundle-nxp && npm run watch",
    "watch": "concurrently -r npm:watch-*",
    "watch-ext": "webpack --config src/extension/webpack.config.js --mode development --watch --stats-error-details",
    "watch-panel": "webpack --config src/panel/webpack.config.js --mode development --watch --stats-error-details"
  },
  "devDependencies": {
    "@cityofzion/neon-core": "^5.0.0-next.12",
    "@types/crypto-js": "^4.0.1",
    "@types/glob": "^7.1.3",
    "@types/gulp": "^4.0.7",
    "@types/node": "^14.14.39",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/temp": "^0.9.0",
    "@types/vscode": "^1.51.0",
    "@types/which": "^2.0.0",
    "bignumber.js": "^9.0.1",
    "bitset": "^5.1.1",
    "buffer": "^6.0.3",
    "concurrently": "^6.0.2",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.0.0",
    "extract-zip": "^2.0.1",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "jsonc-parser": "^3.0.0",
    "nerdbank-gitversioning": "^3.4.244",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "shx": "^0.3.3",
    "stream-browserify": "^3.0.0",
    "temp": "^0.9.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vsce": "^2.9.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.10.0",
    "wget-improved": "^3.2.1",
    "which": "^2.0.2"
  }
}
